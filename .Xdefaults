! Force DPI
Xft.dpi: 139

! Disable Keycap Picture insert mode:
URxvt.iso14755: false
URxvt.iso14755_52: false

! URxvt.perl-ext-common: default,matcher,resize-font
URxvt.perl-ext-common: default,matcher,resize-font,selection-to-clipboard
URxvt.urlLauncher: firefox
URxvt.url-launcher: firefox
URxvt.matcher.button: 1
!urxvt*cursorUnderline: false
!URxvt.cursorColor: #ffff00
!urxvt*cursorUnderline: true
! URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

/* URxvt.keysym.M-u: perl:url-select:select_next */
/* URxvt.url-select.launcher: /usr/bin/xdg-open */
/* URxvt.url-select.underline: true */

URxvt.foreground: white
URxvt.depth: 32
! URxvt*background: rgba:0000/0000/0000/CCCC
URxvt*background: rgba:0000/0000/0000/AAAA
! URxvt.fading: 40
! URxvt.fadeColor: [0]black

URxvt.saveLines:32767
! URxvt.scrollBar: false

! URxvt.keysym.S-Return: <C-Return>

! URxvt.color4: #3456ac

URxvt*color4: #567B94
URxvt*color12: #7CB2D6
! URxvt*color4: #FFFFFF
! URxvt*color12: #FFFFFF

URxvt.bell-command: notify-send "Beep, Beep"

